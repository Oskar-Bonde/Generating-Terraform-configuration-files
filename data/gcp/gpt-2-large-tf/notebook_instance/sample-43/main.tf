terraform {
  required_providers {
    google = {
      source = "hashicorp/google"
    }
  }
}

# google provider block with only region set to europe north1-standard-1
resource "google_compute_instance" "name_0" {
  name = "my-vm"
  machine_type = "f1-micro"

  network_interface {
    network = "default"    
  }
}

