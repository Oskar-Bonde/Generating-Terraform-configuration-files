terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
    }
  }
}

# Provider Block AWS
provider "aws" {
  profile = "default"
  region  = "us-east-1"
}

# Create variable with default value
variable "name_0" {
  default = "terraform-example"
}

# variable block with default value
variable "name_1" {
  default = "t2.micro"
}

# make variable
variable "name_2" {
  default = "terraform-example"
}

# create variable block
variable "name_3" {
  default = "terraform-example"
}

# Create Security Group
