terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
    }
  }
}

# Provider Block AWS
provider "aws" {
  profile = "default"
  region  = "us-east-1"
}

# Create variable 
variable "name_0" {
  description = "VPC ID"
  default     = "vpc-xxxxxx"
}

# variable block
variable
variable "name_1" {
  description = "Subnet ID"
  default     = "subnet-xxxxxx"
}

# make variable
variableresource "aws_instance" "instance" {
  ami           = "ami-0ff8a91507f77f867"
  instance_type = "t2.micro"
}

